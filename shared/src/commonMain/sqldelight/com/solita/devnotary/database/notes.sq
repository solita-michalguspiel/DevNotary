CREATE TABLE note(
note_id TEXT NOT NULL UNIQUE,
title TEXT NOT NULL,
content TEXT NOT NULL,
date_time TEXT NOT NULL,
color TEXT NOT NULL
);

insert:
INSERT INTO note(note_id,title,content,date_time,color)
VALUES(?,?,?,?,?);

selectAll:
SELECT *
FROM note;

update:
UPDATE note
SET title = :title, content = :content,color = :color
WHERE note_id = :note_id;

delete:
DELETE FROM note WHERE(note_id) IN (?);


deleteAll_TEST:
